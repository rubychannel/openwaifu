#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brayanneves/clawd/openclaw/node_modules:/Users/brayanneves/clawd/node_modules:/Users/brayanneves/node_modules:/Users/node_modules:/node_modules:/Users/brayanneves/clawd/openclaw/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brayanneves/clawd/openclaw/node_modules:/Users/brayanneves/clawd/node_modules:/Users/brayanneves/node_modules:/Users/node_modules:/node_modules:/Users/brayanneves/clawd/openclaw/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openclaw/openclaw.mjs" "$@"
else
  exec node  "$basedir/../openclaw/openclaw.mjs" "$@"
fi
